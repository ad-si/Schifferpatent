extends layout

block prepend title
	| #{ page.title + " | "}

block prepend content
	#questions
		!= page.html

		table#questionstable
			- page.metadata.questions.forEach(function(item, index){

					tr
						td= index + 1
						td= item.q
						td= item.a
			- })

		date = new Date(page.date).toISOString().substr(0,10)
		hr
		#modified.
			Letzte Bearbeitung: #{date}